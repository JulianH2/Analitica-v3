              {"target": "clientes_delta", "formula": "lambda r: (r['clientes_servidos'] - r['clientes_servidos_anterior']) / r['clientes_servidos_anterior'] if r.get('clientes_servidos_anterior') else 0"}
          ]
      },

      "utilizacion_flota": {
          "dimensions": [],
          "metrics": [["viajes", "viajes_cargados"]],
          "columns": [
              {
                  "target": "utilizacion_val", 
                  "formula": "lambda r: (r['viajes_cargados'] / r['viajes'] * 100) if r.get('viajes') else 0"
              }
          ]
      }
    },
